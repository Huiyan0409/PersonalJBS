<%- include('bootheader') -%>
<%- include('menubar') -%>
    <div class="jumbotron">
        <h1>TutorMatching Profile for <%= user.userName%></h1>
        <img src="<%= user.profilePicURL%>" width="200">
        <hr>
        <p> Welcome <%= user.googlename %>
            with email <%= user.googleemail %>
            You are logged in to TutorMatching.
        </p>
        <p> Last updated on <%= user.lastUpdate %> </p>
        <hr>
        <form method="post" action="/updateProfile">
          Name: <input type="text" name="userName" value="<%= user.userName %>"><br>
          Zipcode: <input type = "text" name = "zipcode" value="<%= user.zipcode %>"><br>
          <input type="submit" class="btn btn-lg btn-primary">
        </form>
    </div>

<%- include('bootfooter') -%>
